<div class="span12">
  <table style="margin: 0 auto"><tbody><tr>
    <td class="arrow_col">
        <div id="l_next">
          <div><img src="/img/larrow.png" width="22" height="10"/></div>

          <span class="pseudo">Zenburn</span>
        </div>
    </td>

    <td>
      <div class="code_plate">
        <div class="color_chooser">
          <span class="pseudo">
            <div id="plate_fg"><span class="color_square"></span>#ffffff</div>
          </span>

          <span class="pseudo">
            <div id="plate_bg"><span class="color_square"></span>#000000</div>
          </span>
        </div>
        <%= code_plates.first.render %>
        <div class="code_plate_name"><%= code_plates.first.name %></div>
        <div class="pt_plate_logo">Pry Theme</div>
      </div>
    </td>

    <td class="arrow_col">
      <div id="r_next">
        <div><img src="/img/rarrow.png" width="22" height="10"/></div>

        <span class="pseudo">
          Pry Classic 256
        </span>
      </div>
    </td>
  </tr></tbody></table>
</div>
